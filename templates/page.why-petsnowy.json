        "2f9df16b-1348-45a7-98a9-4078ff9800c2": {
          "type": "title",
          "settings": {
            "image_first": false,
            "title": "The World's Most Trusted Smart Pet Care Innovator",
            "subtitle": "",
            "richtext": "<p>PetSnowy has a self-owned production supply chain with over 34 years of experience. We own 6 production bases worldwide, which encompass an impressive 180,000 square kilometers. <\/p>",
            "image": "shopify:\/\/shop_images\/petsnowy_park.jpg"
